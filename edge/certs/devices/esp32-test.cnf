prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = v3_req

[dn]
C=NO
ST=Oslo
L=Oslo
O=SafeSignal-Dev
OU=Edge
CN=device-esp32-001

[v3_req]
keyUsage = critical, digitalSignature, keyEncipherment
